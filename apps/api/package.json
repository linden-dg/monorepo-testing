{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "bin": {
    "backend": "bin/backend.ts"
  },
  "scripts": {
    "dev": "nodemon -e ts -w . -x ts-node ./src/main.ts",
    "build": "tsup",
    "start": "npm run dev",
    "lint": "eslint \"src/**/*.ts*\"",
    "clean": "rimraf .turbo && rimraf node_modules && rimraf cdk.out",
    "cdk": "cdk",
    "cdk:synth": "cdk synth --profile dev",
    "cdk:deploy": "cdk deploy --profile dev"
  },
  "dependencies": {
    "@spicy-soup/trpc": "*",
    "constructs": "^10.1.150",
    "express": "^4.17.1",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@spicy-soup/cdk": "*",
    "@spicy-soup/eslint-config": "*",
    "@spicy-soup/tsconfig": "*",
    "@types/aws-lambda": "^8.10.108",
    "@types/express": "^4.17.12",
    "aws-cdk": "^2.50.0",
    "aws-cdk-lib": "^2.50.0",
    "aws-lambda": "^1.0.7",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^4.5.3"
  }
}
